}
ul.menu li.leaf
{
  background: transparent url(images/menu-leaf.gif) no-repeat 3px 10px; /* LTR */
}
ol li {
  padding: 0 0 .3em;
  margin-left: 2em; /* LTR */
}

ul li.expanded {
  background: transparent url(images/menu-expanded.gif) no-repeat 3px 10px; /* LTR */
}

