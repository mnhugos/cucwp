/**
 * Generic elements
 */

body 
{
  margin: 0;
  margin-top: 2px;
  padding: 0;
  background: #FFFFFF;
  font: 12px/140% Verdana, sans-serif;
  color: #000000;
  text-align: center;
}

li.menu-135
{

}
/* turn off displaying userName & the word "Navigation" in side menu */
#block-user-1 h2 {
     display: none;
}
